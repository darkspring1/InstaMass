{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": ["off", "windows"],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "no-debugger": "warn",
    "comma-dangle": "off",
    "max-len": ["error", 120, 2, {
      "ignoreUrls": true,
      "ignoreStrings": false,
      "ignoreTemplateLiterals": false,
      "ignoreRegExpLiterals": false
    }],
    "react/prop-types": "off"
  },
  "globals": {
        "NODE_ENV": true,
        "ReactDOM": true,
        "React": true
    }
}
